<!DOCTYPE html>
<html ng-app="cargoApp" ng-cloak>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Asencoex Cargo</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/Animate.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet">
</head>
<body class="fadeIn animated">
    <div class="wrapper">
        <div class="sidebar" data-color="grey" data-image="http://@HttpContext.Current.Request.Url.Authority/Images/pexels-photo-122164.jpeg">
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="@Url.Action("Index", "Home")" class="simple-text">
                        <i class="fa fa-truck fa-2x"></i>
                    </a>
                </div>

                <ul class="nav">
                    <li id="btn-1" onmouseover="animateSideBar(this)" onmouseout="desanimateSideBar(this)">
                        <a data-toggle="collapse" data-target="#warehouseSubmenu" aria-expanded="false">
                            <i class="pe-7s-box2"></i>
                            <p>asdas <span translate="WAREHOUSE"></span></p>
                        </a>
                        <ul class="nav collapse" id="warehouseSubmenu" role="menu" aria-labelledby="btn-1">
                            <li onmouseover="animateSideBar(this)" onmouseout="desanimateSideBar(this)">
                                <a href="@Url.Action("Create","Courier")">
                                    <i class="fa fa-truck"></i>
                                    <p>Create courier</p>
                                </a>
                            </li>                            
                            <li onmouseover="animateSideBar(this)" onmouseout="desanimateSideBar(this)">
                                <a href="@Url.Action("Index","warehouse")">
                                    <i class="fa fa-truck"></i>
                                    <p>warehouse list</p>
                                </a>
                            </li>                            
                        </ul>
                    </li>

                    <li onmouseover="animateSideBar(this)" onmouseout="desanimateSideBar(this)">
                        <a href="@Url.Action("Parametrization","Home")">
                            <i class="fa fa-sliders"></i>
                            <p><span translate="PARAMETRIZATION"></span></p>
                        </a>
                    </li>
                    <li onmouseover="animateSideBar(this)" onmouseout="desanimateSideBar(this)">
                        <a href="@Url.Action("Index","CompanyAccount")">
                            <i class="fa fa-users"></i>
                            <p><span translate="COMPANIESACCOUNTS"></span></p>
                        </a>
                    </li>
                    <li onmouseover="animateSideBar(this)" onmouseout="desanimateSideBar(this)">
                        <a href="@Url.Action("Index","EventBlog")">
                            <i class="pe-7s-note2"></i>
                            <p>Blog</p>
                        </a>
                    </li>
                </ul>
            </div>
        </div>

        <div class="main-panel">
            <nav class="navbar navbar-default navbar-fixed">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="#">Asencoex</a>
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-left">
                            @*<li>
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                        <i class="fa fa-dashboard"></i>
                                    </a>
                                </li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                        <i class="fa fa-globe"></i>
                                        <b class="caret"></b>
                                        <span class="notification">5</span>
                                    </a>
                                    <ul class="dropdown-menu">
                                        <li><a href="#">Notification 1</a></li>
                                        <li><a href="#">Notification 2</a></li>
                                        <li><a href="#">Notification 3</a></li>
                                        <li><a href="#">Notification 4</a></li>
                                        <li><a href="#">Another notification</a></li>
                                    </ul>
                                </li>
                                <li>
                                    <a href="">
                                        <i class="fa fa-search"></i>
                                    </a>
                                </li>*@
                        </ul>

                        <ul class="nav navbar-nav navbar-right">
                            @Html.Partial("_LoginPartial")
                           
                        </ul>
                    </div>
                </div>
            </nav>


            <div class="content container">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>

            <footer class="footer">
                <div class="container-fluid">
                    <nav class="pull-left">
                        <ul>
                            <li><a href="@Url.Action("About", "Pages")">About</a></li>
                            <li><a href="@Url.Action("Chat", "Pages")">Chat</a></li>
                            <li><a href="@Url.Action("Contact", "Pages")">Contact</a></li>
                            <li><a href="@Url.Action("Advertise", "Pages")">Advertise</a></li>
                            <li><a href="@Url.Action("WriteForUs", "Pages")">Write for us</a></li>
                            <li><a href="@Url.Action("Search", "Post")">Search</a></li>
                            <li><a href="@Url.Action("Privacy", "Pages")">Privacy</a></li>
                            <li><a href="@Url.Action("Terms", "Pages")">Terms</a></li>
                            <li><a href="@Url.Action("Subscribe", "Pages")">Subscribe</a></li>
                        </ul>
                    </nav>
                    <p class="copyright pull-right">
                        &copy; @DateTime.Now.Year @ViewBag.SiteName. All Rights Reserved.
                    </p>
                </div>
            </footer>


        </div>
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @*@Scripts.Render("~/bundles/angular")*@

        <script src="~/Scripts/angular.min.js"></script>
        <script src="~/Scripts/app/directives/angular-sanitize.js"></script>
        <script src="~/Scripts/app/directives/angular-selectize.js"></script>
        <script src="~/Scripts/app/directives/angular-translate.js"></script>
        <script src="~/Scripts/app/directives/datetime-picker.js"></script>
        <script src="~/Scripts/app/directives/datetime-picker.tpls.js"></script>
        <script src="~/Scripts/app/directives/ng-file-upload-shim.min.js"></script>
        <script src="~/Scripts/app/directives/ng-file-upload.js"></script>

        <script src="~/Scripts/app/app.js"></script>
        <script src="~/Scripts/app/components/ui-bootstrap-tpls-2.1.3.min.js"></script>
        <script src="~/Scripts/app/controllers/courier/courierController.js"></script>
        <script src="~/Scripts/app/controllers/delivery/deliveryController.js"></script>
        <script src="~/Scripts/app/controllers/unitType/unitTypeController.js"></script>
        <script src="~/Scripts/app/controllers/blogController.js"></script>
        <script src="~/Scripts/app/controllers/branchController.js"></script>
        <script src="~/Scripts/app/controllers/classificationController.js"></script>
        <script src="~/Scripts/app/controllers/companyAccountController.js"></script>
        <script src="~/Scripts/app/controllers/conditionController.js"></script>
        <script src="~/Scripts/app/controllers/countryController.js"></script>
        <script src="~/Scripts/app/controllers/destinationController.js"></script>
        <script src="~/Scripts/app/controllers/LayoutController.js"></script>
        <script src="~/Scripts/app/controllers/notificationController.js"></script>
        <script src="~/Scripts/app/controllers/originController.js"></script>
        <script src="~/Scripts/app/controllers/PostController.js"></script>
        <script src="~/Scripts/app/controllers/warehouseClassificationController.js"></script>
        <script src="~/Scripts/app/controllers/warehouseController.js"></script>
        
        <script src="~/Scripts/jquery.signalR-2.2.1.min.js"></script>
        <script src="~/signalr/hubs"></script>
        <script>
            function animateSideBar(obj) {
                $(obj).children().children().addClass("swing animated");
            }
            function desanimateSideBar(obj) {
                $(obj).children().children().removeClass("swing animated");
            }
        </script>
        @RenderSection("scripts", required: false)
    </div>
</body>

</html>
